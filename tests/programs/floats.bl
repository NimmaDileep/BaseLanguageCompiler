

fun getArea(radius: Float) {
    var pi = 3.14159;

    return pi * (radius * radius);
}

fun semiCircleArea(radius: Float) {
    return getArea(radius) / 2; // using division
}

var area = getArea(5.2);

print(area);

print(area + 50); // usage with ints

print(semiCircleArea(5.2)); // semicircle area