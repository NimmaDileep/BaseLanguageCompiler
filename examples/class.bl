class Dog {
    var name: String;
    var age: Int;

    fun isYoung(thresh: Int) -> Boolean {
        print(thresh);
        if(self.age < thresh) {
            return true;
        } else {
            return false;
        }
    }

    fun bark() -> Unit {
        print(concat(self.name, " says woof"));
    }

    var isCool: Boolean;
}

var x: Dog = Dog("Fido", 5, false);

print(x.isYoung(4));
print(x.isYoung(6));

print(x.isCool);
x.bark();

var y: Dog = x;
x.name = "Bobby";
y.bark();
